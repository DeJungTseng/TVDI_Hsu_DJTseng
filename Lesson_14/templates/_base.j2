  </div>
  <div class="container">
    <main>
    {% block main%}

    {% endblock %}
    
    </main>
  </div>

  <div class="container">
      {% import 'macros.j2' as macros %}
      {{macros.get_footer(color='grey')}}
    </div>

    
    <script src="{{url_for('static',filename='js/bootstrap.bundle.min.js')}}"></>

    </body>
</html>